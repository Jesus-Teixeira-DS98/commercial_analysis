{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "helpdeskDataFactory"
		},
		"Data Lake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Data Lake'"
		},
		"Azure SQL DW_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Azure SQL DW'"
		},
		"Data Lake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://helpdeskazure1998.dfs.core.windows.net/"
		},
		"GitHub Filles 2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Jesus-Teixeira-DS98/commercial_analysis/main/Vendas.csv"
		},
		"Github Files_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Jesus-Teixeira-DS98/commercial_analysis/main/Metas.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "helpdeskDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Data Lake_accountKey": {
						"value": "[parameters('Data Lake_accountKey')]"
					},
					"Azure SQL DW_connectionString": {
						"value": "[parameters('Azure SQL DW_connectionString')]"
					},
					"Data Lake_properties_typeProperties_url": {
						"value": "[parameters('Data Lake_properties_typeProperties_url')]"
					},
					"GitHub Filles 2_properties_typeProperties_url": {
						"value": "[parameters('GitHub Filles 2_properties_typeProperties_url')]"
					},
					"Github Files_properties_typeProperties_url": {
						"value": "[parameters('Github Files_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "helpdeskDataFactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'helpdeskDataFactory_ArmTemplate_0')]"
			]
		}
	]
}