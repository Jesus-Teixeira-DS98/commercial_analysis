{
	"name": "Datalake to DW",
	"properties": {
		"activities": [
			{
				"name": "Load Data into dProduto",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "Truncate Table \ndbo.dProduto",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "cdProduto",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "cdProduto",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Produto",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Produto",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Grupo Produto",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Grupo_Produto",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Linha Produto",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Linha_Produto",
									"type": "String",
									"physicalType": "varchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "dProdutotoDW",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}